<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_ProcessoMEI"
                  targetNamespace="http://bpmn.io/schema/bpmn">

  <bpmn:process id="ProcessoAberturaMEI" name="Processo de Abertura MEI" isExecutable="true">

    <!-- INÍCIO -->
    <bpmn:startEvent id="StartEvent_SolicitacaoMEI" name="Solicitação de Abertura MEI"/>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_SolicitacaoMEI" targetRef="Task_SolicitarGovBr"/>

    <!-- TAREFA: SOLICITAR CONTA GOV.BR -->
    <bpmn:userTask id="Task_SolicitarGovBr" name="Solicitar conta gov.br do cliente"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_SolicitarGovBr" targetRef="Task_DefinirAtividade"/>

    <!-- TAREFA: DEFINIR ATIVIDADE E ENDEREÇO -->
    <bpmn:task id="Task_DefinirAtividade" name="Definir atividade (objeto social) e endereço"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_DefinirAtividade" targetRef="Gateway_EnderecoValido"/>

    <!-- GATEWAY: VERIFICAR SE ENDEREÇO É VÁLIDO -->
    <bpmn:exclusiveGateway id="Gateway_EnderecoValido" name="Endereço válido?" default="Flow_5"/>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Gateway_EnderecoValido" targetRef="Task_CorrigirEndereco">
      <bpmn:conditionExpression><![CDATA[${enderecoValido == false}]]></bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Gateway_EnderecoValido" targetRef="IntermediateEvent_DocumentosOk"/>

    <!-- TAREFA: CORRIGIR ENDEREÇO -->
    <bpmn:userTask id="Task_CorrigirEndereco" name="Corrigir endereço do MEI"/>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_CorrigirEndereco" targetRef="Gateway_EnderecoValido"/>

    <!-- EVENTO INTERMEDIÁRIO: DOCUMENTOS OK -->
    <bpmn:intermediateCatchEvent id="IntermediateEvent_DocumentosOk" name="Documentos Validados">
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration>PT5S</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="IntermediateEvent_DocumentosOk" targetRef="Task_GerarProcesso"/>

    <!-- TAREFA: GERAR PROCESSO ELETRÔNICO -->
    <bpmn:serviceTask id="Task_GerarProcesso" name="Gerar processo eletrônico na Junta Comercial"/>
    <bpmn:sequenceFlow id="Flow_8" sourceRef="Task_GerarProcesso" targetRef="Task_GerarTaxa"/>

    <!-- TAREFA: GERAR TAXA -->
    <bpmn:task id="Task_GerarTaxa" name="Gerar taxa da junta comercial"/>
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Task_GerarTaxa" targetRef="Task_IntegrarAssinatura"/>

    <!-- TAREFA: INTEGRAR PARA ASSINATURA -->
    <bpmn:task id="Task_IntegrarAssinatura" name="Integrar processo para assinatura"/>
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_IntegrarAssinatura" targetRef="Task_AvisarCliente"/>

    <!-- TAREFA: AVISAR CLIENTE -->
    <bpmn:sendTask id="Task_AvisarCliente" name="Avisar cliente que está pronto para assinatura"/>
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Task_AvisarCliente" targetRef="Task_AcompanharAssinatura"/>

    <!-- TAREFA: ACOMPANHAR ASSINATURA -->
    <bpmn:userTask id="Task_AcompanharAssinatura" name="Acompanhar assinatura do cliente"/>
    <bpmn:sequenceFlow id="Flow_12" sourceRef="Task_AcompanharAssinatura" targetRef="Gateway_Assinado"/>

    <!-- GATEWAY: VERIFICAR SE FOI ASSINADO -->
    <bpmn:exclusiveGateway id="Gateway_Assinado" name="Documento assinado?" default="Flow_14"/>
    <bpmn:sequenceFlow id="Flow_13" sourceRef="Gateway_Assinado" targetRef="IntermediateEvent_AguardarAssinatura">
      <bpmn:conditionExpression><![CDATA[${documentoAssinado == false}]]></bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_14" sourceRef="Gateway_Assinado" targetRef="Task_EnviarJunta"/>

    <!-- EVENTO INTERMEDIÁRIO: AGUARDAR ASSINATURA -->
    <bpmn:intermediateCatchEvent id="IntermediateEvent_AguardarAssinatura" name="Aguardar Assinatura">
      <bpmn:timerEventDefinition>
        <bpmn:timeDuration>PT10S</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:sequenceFlow id="Flow_15" sourceRef="IntermediateEvent_AguardarAssinatura" targetRef="Task_AcompanharAssinatura"/>

    <!-- TAREFA: ENVIAR PARA JUNTA -->
    <bpmn:serviceTask id="Task_EnviarJunta" name="Enviar processo para a junta comercial"/>
    <bpmn:sequenceFlow id="Flow_16" sourceRef="Task_EnviarJunta" targetRef="Task_EmitirRLE"/>

    <!-- TAREFA: EMITIR RLE -->
    <bpmn:task id="Task_EmitirRLE" name="Emitir Registro de Licenciamento (RLE)"/>
    <bpmn:sequenceFlow id="Flow_17" sourceRef="Task_EmitirRLE" targetRef="Task_EnviarRLE"/>

    <!-- TAREFA: ENVIAR RLE AO CLIENTE -->
    <bpmn:sendTask id="Task_EnviarRLE" name="Enviar RLE ao cliente"/>
    <bpmn:sequenceFlow id="Flow_18" sourceRef="Task_EnviarRLE" targetRef="ParallelGateway_Finalizacao"/>

    <!-- GATEWAY PARALELO: TAREFAS FINAIS -->
    <bpmn:parallelGateway id="ParallelGateway_Finalizacao" name="Executar tarefas finais"/>
    <bpmn:sequenceFlow id="Flow_19" sourceRef="ParallelGateway_Finalizacao" targetRef="Task_EnquadrarSimples"/>
    <bpmn:sequenceFlow id="Flow_20" sourceRef="ParallelGateway_Finalizacao" targetRef="Task_CertificadoDigital"/>

    <!-- TAREFA: ENQUADRAR NO SIMPLES -->
    <bpmn:task id="Task_EnquadrarSimples" name="Enquadrar no Simples Nacional (30 dias)"/>
    <bpmn:sequenceFlow id="Flow_21" sourceRef="Task_EnquadrarSimples" targetRef="ParallelGateway_Convergencia"/>

    <!-- TAREFA: CERTIFICADO DIGITAL -->
    <bpmn:task id="Task_CertificadoDigital" name="Encaminhar para certificação digital A1"/>
    <bpmn:sequenceFlow id="Flow_22" sourceRef="Task_CertificadoDigital" targetRef="ParallelGateway_Convergencia"/>

    <!-- GATEWAY PARALELO: CONVERGÊNCIA -->
    <bpmn:parallelGateway id="ParallelGateway_Convergencia" name="Convergir tarefas"/>
    <bpmn:sequenceFlow id="Flow_23" sourceRef="ParallelGateway_Convergencia" targetRef="IntermediateEvent_ProcessoConcluido"/>

    <!-- EVENTO INTERMEDIÁRIO: PROCESSO CONCLUÍDO -->
    <bpmn:intermediateThrowEvent id="IntermediateEvent_ProcessoConcluido" name="MEI Registrado">
      <bpmn:messageEventDefinition/>
    </bpmn:intermediateThrowEvent>
    <bpmn:sequenceFlow id="Flow_24" sourceRef="IntermediateEvent_ProcessoConcluido" targetRef="EndEvent_MEIAberto"/>

    <!-- FIM -->
    <bpmn:endEvent id="EndEvent_MEIAberto" name="MEI Aberto com Sucesso"/>

  </bpmn:process>

